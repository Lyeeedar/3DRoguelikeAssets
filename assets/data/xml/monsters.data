<!-- 
	This is the list of different attributes a creature can be assigned in the evolver. Consists of the subgroups:
		- MIND
		- VISUAL
		- SKIN
		- BONES
		- MUSCLES
		- HANDS
-->
<attributes>

	<!-- The brain of the creature. Affects its eventual AI (modified by its IQ). Does not affect drops. -->
	<MIND>

	</MIND>

	<!-- The creatures ability to see. Affects the stimuli entered into the AI. Does not affect drops. -->
	<VISUAL>

	</VISUAL>

	<!-- The skin of the creature. Affects its health, defenses and weight. Also affects drops. -->
	<SKIN>
		<exoskeleton>

		</exoskeleton>

		<feathers>

		</feathers>

		<scales>

		</scales>

		<fur>

			<soft_fur>
				<calories>0</calories>
				<description>
					<overall>Some soft fur. Does not provide any protection.</overall>
				</description>

				<stats>
					<WEIGHT>0</WEIGHT>

					<HEALTH>0</HEALTH>
					<ELE_DEFENSES>
						<FIRE>0</FIRE>
						<WATER>0</WATER>
						<AIR>0</AIR>
						<WOOD>0</WOOD>
						<METAL>0</METAL>
						<AETHER>0</AETHER>
						<VOID>0</VOID>
					</ELE_DEFENSES>
					<DEFENSES>
						<PIERCE>0</PIERCE>
						<IMPACT>0</IMPACT>
						<TOUCH>0<TOUCH>
					</DEFENSES>
					<STRENGTH>0</STRENGTH>

				</stats>
			</soft_fur>

		</fur>

		<leather>

			<soft_leather>
				<calories>0</calories>
				<description>
					<overall>Some soft, leathery skin. Does not provide any protection.</overall>
				</description>

				<stats>
					<WEIGHT>0</WEIGHT>

					<HEALTH>0</HEALTH>
					<ELE_DEFENSES>
						<FIRE>0</FIRE>
						<WATER>0</WATER>
						<AIR>0</AIR>
						<WOOD>0</WOOD>
						<METAL>0</METAL>
						<AETHER>0</AETHER>
						<VOID>0</VOID>
					</ELE_DEFENSES>
					<DEFENSES>
						<PIERCE>0</PIERCE>
						<IMPACT>0</IMPACT>
						<TOUCH>0<TOUCH>
					</DEFENSES>
					<STRENGTH>0</STRENGTH>

				</stats>
			</soft_leather>

		</leather>

		<clothing>

		</clothing>

		<armour_primitive>

		</armour_primitive>

		<armour_magical>

		</armour_magical>

		<armour_mechanical>

		</armour_mechanical>

	</SKIN>

	<!-- The bones of the creature. Affects health, weight and damage defense. Affects drops. -->
	<BONES>

		<normal>
			<basic_bones>

				<calories>0</calories>

				<stats>
					<WEIGHT>0</WEIGHT>

					<HEALTH>0</HEALTH>

					<DEFENSES>
						<PIERCE>0</PIERCE>
						<IMPACT>0</IMPACT>
						<TOUCH>0<TOUCH>
					</DEFENSES>

				</stats>
			</basic_bones>
		</normal>
	</BONES>

	<!-- The muscles of the creature. Affects strength (damage) and weight. Does not affect drops. -->
	<MUSCLES>

		<normal>
			<basic_muscles>
				<calories>0</calories>
				<stats>
					<WEIGHT>0</WEIGHT>

					<STRENGTH>0</STRENGTH>

				</stats>
			</basic_muscles>
		</normal>

	</MUSCLES>

	<!-- The attacks of the creature. Affects drops. -->
	<ATTACK>
		<teeth>
			<weak_bite>
				<calories>0</calories>
				<stats>
					<type>teeth</type>
					<STRENGTH>0</STRENGTH>

				</stats>
			</weak_bite>
		</teeth>

		<claws>

		</claws>

		<tail>
		</tail>

		<weapon_primitive>

		</weapon_primitive>

		<weapon_magical>

		</weapon_magical>

		<weapon_mechanical>

		</weapon_mechanical>

	</ATTACK>

</attributes>


<!-- This is the list of different monster types in the game. A monster type consists of definitions of creatures, and a list of where the creature appears in the difficulty heirachy (0-9) -->
<monsters>

	<!-- The Rodent type. Rats and suchlike -->
	<RODENT>

		<depth_min>1</depth_min>
		<depth_max>10</depth_max>

		<monster_type>group</monster_type>

		<creatures>
			<MOUSE>
				<visual>
					<description>A mouse. It seems hungry.</description>
