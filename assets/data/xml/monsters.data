						<scaling>1.0</scaling>
						<type>eyes</type>
					</VISUAL>

					<SKIN>
						<scaling>1.0</scaling>
						<type>fur</type>
					</SKIN>

					<BONES>
						<scaling>1.0</scaling>
						<type>normal</type>
					</BONES>

					<MUSCLES>
						<scaling>2.0</scaling>
