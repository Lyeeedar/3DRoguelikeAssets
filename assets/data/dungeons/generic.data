<?xml version="1.0"?>

<ROOM_DEFINITIONS>

<!-- START rooms. Size 1-3 -->
<START>
	<ladder>
		<width>1</width>
		<height>1</height>

		<definitions>
			<symbol>
				<char>L</char>
				<type>stair_up</type>
				<visible>true</visible>
				<description>A Ladder. It leads up.</description>
				<model>
					<type>cube</type>
					<name></name>
					<dimensions>
						<number>3</number>
						<d1>1.0</d1>
						<d2>1.0</d2>
						<d3>1.0</d3>
					</dimensions>
				</model>
				<texture>tex^</texture>
				<colour>
					<red>1.0</red>
					<green>1.0</green>
					<blue>1.0</blue>
				</colour>
			</symbol> 
		</definitions>

		<room>
			<row1>L</row1>
		</room>
	</ladder>

</START>

<!-- END rooms. Size 1-3 -->
<END>

</END>

<!-- MAIN rooms. Size 15-25 -->
<MAIN>

</MAIN>

<!-- SPECIAL rooms. Size 5-20 -->
<SPECIAL>

</SPECIAL>

<!-- OTHER rooms. Size 3-8 -->
<OTHER>

	<monster_camp>

		<width>5</width>
		<height>5</height>

		<definitions>

			<symbol>
				<char>l</char>
				<type>light_fire</type>
				<visible>false</visible>
			</symbol>

			<symbol>
				<char>0</char>
				<type>spawner_0</type>
				<visible>false</visible>
			</symbol>

			<symbol>
				<char>1</char>
				<type>spawner_1</type>
				<visible>false</visible>
			</symbol> 

		</definitions>

		<room>
			<row1>.....</row1>
			<row2>.0.0.</row2>
			<row3>..l.0</row3>
			<row4>.0.1.</row4>
			<row5>.....</row5>
		</room>

	</monster_camp>

</OTHER>


</ROOM_DEFINITIONS>